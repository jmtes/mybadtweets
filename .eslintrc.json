{
  "extends": ["airbnb", "prettier", "plugin:node/recommended"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "warn",
    "no-console": "off",
    "func-names": "off",
    "no-process-exit": "off",
    "node/no-unsupported-features/es-syntax": "off",
    "no-underscore-dangle": "off"
  }
}
